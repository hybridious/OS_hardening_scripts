function f_pre {

  export TERM=linux
  export DEBIAN_FRONTEND=noninteractive

  if [[ $VERBOSE == "Y" ]]; then
    APTFLAGS='-y'
  else
    APTFLAGS='-qq -y'
  fi

  APT="apt-get $APTFLAGS"

  if [[ $CHANGEME == "" ]]; then
    echo "Please read the code. Exiting."
    echo
    exit 1
  fi

  if [ $EUID -ne 0 ]; then
    echo "Not root or not enough privileges. Exiting."
    echo
    exit 1
  fi

  if ! lsb_release -i | grep 'Ubuntu'; then
    echo "Ubuntu only. Exiting."
    echo
    exit 1
  fi

  i="0"
}
